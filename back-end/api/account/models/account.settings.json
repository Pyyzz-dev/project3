{
  "kind": "collectionType",
  "collectionName": "accounts",
  "info": {
    "name": "Account"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Username": {
      "type": "string"
    },
    "Password": {
      "type": "password"
    },
    "Email": {
      "type": "email"
    },
    "Phone": {
      "type": "string"
    },
    "role": {
      "plugin": "users-permissions",
      "model": "role"
    },
    "posts": {
      "collection": "post"
    },
    "comment": {
      "model": "comment",
      "via": "account"
    }
  }
}
